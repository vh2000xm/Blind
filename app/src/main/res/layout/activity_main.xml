<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:theme="@style/Theme.AppCompat.NoActionBar"
    tools:context="com.example.innoz.iotapplication.MainActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="70dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"></LinearLayout>

    <LinearLayout
        android:layout_width="198dp"
        android:layout_height="208dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="86dp">

        <LinearLayout
            android:layout_width="194dp"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="176dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:visibility="visible"
                app:srcCompat="@drawable/livingroom" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="195dp"
        android:layout_height="195dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="302dp">

        <LinearLayout
            android:layout_width="194dp"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:visibility="visible"
                app:srcCompat="@drawable/kichen" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="56dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="503dp"></LinearLayout>

    <LinearLayout
        android:layout_width="174dp"
        android:layout_height="208dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="86dp">

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="0sp"
            android:scaleType="centerCrop"
            android:visibility="visible"
            app:srcCompat="@drawable/small_room" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="165dp"
        android:layout_height="193dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="302dp">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="0sp"
            android:scaleType="centerCrop"
            android:visibility="visible"
            app:srcCompat="@drawable/add_room" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
